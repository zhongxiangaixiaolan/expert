# 开发环境配置

# 应用信息
VITE_APP_TITLE=达人接单管理系统 - 开发环境
VITE_APP_ENV=development

# API配置 - 开发环境使用Vite代理，直接使用相对路径
VITE_API_BASE_URL=
VITE_API_TIMEOUT=30000

# 静态资源配置 - 开发环境通过Vite代理访问
VITE_STATIC_BASE_URL=

# 开发工具配置
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_CONSOLE_LOG=true
VITE_ENABLE_VCONSOLE=false

# 调试配置
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# 热更新配置
VITE_HMR_PORT=3000
